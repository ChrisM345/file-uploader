<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="stylesheet" href="/css/styles.css" />
    <title><%= title %></title>
  </head>
  <body>
    <h1>Folder: <%= title %></h1>
    <%- include("partials/errors.ejs") %>
    <ul>
      <li><a href="/file/<%= folderName %>/uploadFile">Upload File</a></li>
    </ul>

    <h2>Uploaded Files</h2>
    <ul>
      <% files.forEach((file) => { %>
      <li><a href="/file/<%= file.name %>"><%= file.name %></a></li>
      <%}) %>
    </ul>
  </body>
</html>
